/* pages/room-code/room-code.wxss */

.lxy-nav-bar {
  background-color: transparent !important;
}

.group_head {
  padding: 10rpx 30rpx;
}

.group_head .logo {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
}

.group_wraper image {
  width: 100%;
  height: 100%;
}

.look {
  text-align: center;
}

.look_icon {
  width: 40rpx;
  height: 40rpx;
  margin: 0px auto;
}

.look .text {
  font-size: 20rpx;
}

.group_head .info {
  margin-left: 20rpx;
  width: 100%;
}

.group_head .name {
  font-weight: 600;
}

.group_head .level {
  font-size: 24rpx;
}

.group_member {
  padding: 20rpx 30rpx;
}

.group_member .title {
  font-weight: 600;
}

.group_member .ul {
  margin-top: 14rpx;
}

.group_member .ul .li .logo {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 20rpx;
}

.group_member .ul .li .identity {
  color: #000;
  font-size: 22rpx;
}

.price_wraper {
  margin-top: 20rpx;
}

.price_wraper .price_li {
  padding: 15rpx 20rpx;
  width: 220rpx;
  text-align: center;
  background-color: rgba(0, 0, 0, 0.5);
  color: #f7b10a;
}

.price_wraper .price_li .text {
  font-size: 24rpx;
}

.buy_record {
  background-color: transparent !important;
}

.buy_record .text {
  color: #000 !important;
  margin-top: 10rpx;
}

.cover {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 0px auto;
}

.cover image {
  width: 100%;
  height: 100%;
}
.auction_wraper {
  padding-bottom: 100rpx;
}
.auction_ul {
  margin: 10rpx 20rpx;
  background-color: #fff;
  padding: 10rpx 20rpx;
}

.auction_box .title {
  font-weight: 600;
  padding-left: 20rpx;
}

.auction_box .btn_sty {
  background-color: rgba(0, 0, 0, 0.5);
  color: #f7b10a;
  text-align: center;
  font-size: 24rpx;
  margin-top: 10rpx;
  line-height: 48rpx;
}

.top_bar {
  background: #fff;
  height: 70rpx;
  margin: 10rpx 20rpx;
}
.top_bar_li {
  flex: 1;
}
.bar_item {
  display: inline-block;
  padding: 30rpx 16rpx;
  font-size: 28rpx;
  color: #a5a5a5;
}

.top_bar_current {
  background-color: rgba(0, 0, 0, 0.5);
  color: #f7b10a;
}
.equip_ul {
  max-height: 800rpx;
}
.equip_li image {
  width: 100%;
  height: 100%;
}
.equip_li {
  margin: 10rpx 20rpx;
  padding: 10rpx;
  background-color: #fff;
}
.equip_li .icon {
  width: 100rpx;
}
.equip_li .cover {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
}
.equip_li .info {
  width: 500rpx;
  padding-left: 20rpx;
}
.equip_li .fun {
  width: 150rpx;
}
.equip_li .current_price {
  font-size: 24rpx;
  margin-top: 10rpx;
}
.equip_li .level {
  font-size: 22rpx;
  margin-left: 10rpx;
}

.fun_btn {
  position: fixed;
  width: 100%;
  left: 0px;
  bottom: 0px;
}
.fun_btn .bnt1 {
  flex: 1;
  height: 100rpx;
  line-height: 100rpx;
  background-color: rgba(0, 0, 0, 0.9);
  color: #f7b10a;
  text-align: center;
}
.right_border {
  border-right: 1px solid rgba(247, 176, 10, 0.3);
}

/* 房间详细信息 */
.group_box {
  padding: 30rpx;
  max-height: 1082rpx;
}
.group_box .li {
  font-size: 24rpx;
  margin-bottom: 10rpx;
  color: rgba(0, 0, 0, 0.8);
}
.group_box .title {
  width: 200rpx;
  text-align: right;
  padding-right: 20rpx;
  font-size: 26rpx;
  color: #000;
}
/* 成员信息 */
.team_ul {
  height: 1082rpx;
}
.team_li {
  margin: 0rpx;
}
.team_li .info {
  width: 390rpx !important;
  padding-left: 20rpx;
}
.team_li .info text {
  margin-right: 10rpx;
}
.team_li .fun {
  width: 120rpx !important;
  text-align: center;
}
.team_li .btn_sty {
  padding: 4rpx 20rpx;
  display: inline;
  background-color: #eee;
  font-size: 24rpx;
  border-radius: 30rpx;
}

.group_flex {
  flex: 1;
}

/* 添加装备 */
.boss_box {
  height: 1180rpx;
  background-color: #2d2d2d !important;
}

.scroll_h {
  height: 1082rpx;
  background-color: #2d2d2d;
}

.boss_equip_h {
  height: 842rpx;
}

.boss_equip {
  font-size: 24rpx;
}

.ul_box .ul_li {
  background-color: #190f00;
  color: #ffd100;
  text-align: center;
  padding: 20rpx 0px;
  border-bottom: 1px solid #2d2d2d;
}

.eqiop_name {
  width: 190rpx;
}

.eqiop_name .english_name {
  font-size: 22rpx;
}
.eqiop_icon {
  width: 80rpx;
  text-align: center;
}
.eqiop_icon image {
  width: 50rpx;
  height: 50rpx;
}
.l1_eqiop {
  width: 80rpx;
  position: relative;
}

.select_icon {
  width: 80rpx;
}

.icon_sty {
  width: 30rpx;
  height: 30rpx;
  border: 1px solid #fff;
  position: relative;
  border-radius: 5rpx;
  margin-left: 20rpx;
}

.icon_sty image {
  position: absolute;
  top: -5rpx;
  left: -5rpx;
  width: 37rpx;
  height: 37rpx;
}

.set_up {
  width: 90rpx;
  height: 34rpx;
  background-color: #fff;
  margin: 0px auto;
  color: #000;
  text-align: center;
  font-size: 24rpx;
}
.set_boss_equip {
  width: 100%;
  height: 160rpx;
}
.set_boss_equip .set_hook {
  height: 80rpx;
  color: #fff;
  width: 70rpx;
  padding-top: 8rpx;
  box-sizing: border-box;
}
.set_hook text {
  font-size: 22rpx;
  text-align: center;
  padding-top: 8rpx;
  padding-left: 10rpx;
}
.li_qu {
  width: 440rpx;
}
.set_boss_equip .set_boss_equip_btn {
  text-align: center;
  line-height: 80rpx;
  color: #ffd100;
  background-color: rgba(0, 0, 0, 0.6);
}
.auction_type_picker {
  color: #ffd100;
  font-size: 24rpx;
  text-align: left;
}
.drop {
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
}
.eqiop_type {
  width: 100rpx;
  position: relative;
}
.set_up_input {
  width: 140rpx;
}
.equip_popup_title {
  color: #fff;
}

/* 设置时间 */
.floor_li {
  margin-bottom: 30rpx;
}
.set_floor_btn {
  width: 100%;
  line-height: 80rpx;
  text-align: center;
  color: #ffd100;
  background-color: rgba(0, 0, 0, 0.6);
  margin-top: 80rpx;
}
.floor_time {
  margin-left: 30rpx;
}
.floorEquipIpunt {
  border: 1px solid #190f00;
  width: 200rpx;
  font-size: 24rpx;
  margin-left: 30rpx;
  text-align: center;
}
