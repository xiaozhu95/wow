<view class='main-wraper'>
  <view class='nav'>
    <navBar title='房间' background='rgba(0, 0, 0, 0)' color='#fff' back="{{true}}" iconTheme='white' bindback="handlerGohomeClick"></navBar>
  </view>

  <view class="group_wraper">
    <view class="group_head flex_end">
      <view class="logo">
        <image src="{{team_info.avatar}}"></image>
      </view>
      <view class="info flex_y flex_sb">
        <view>
          <view class="name">
            {{team_info.user_role_name}}
          </view>
        </view>
        <view class="look" bindtap="group_popup">
          <view class="look_icon">
            <image src="../../images/look.png"></image>
          </view>
          <view class="text">开团规则</view>
        </view>
      </view>
    </view>
    <view class="group_member">
      <view class="title flex_sb">
        <text>成员信息({{team_list_li.length}})</text>
        <text bindtap="team_popup">管理</text>
      </view>
      <view class="ul flex-row ">
        <view class="li" wx:for="{{team_list_li}}" wx:key="index" wx:if="{{index<7}}">
          <view class="logo">
            <image src="{{item.avatar}}"></image>
          </view>
          <view class="identity">{{item.identityName}}</view>
        </view>
        <view class="li" bindtap="team_popup" wx:if="{{team_list_li.length>7}}">
          ......
        </view>
      </view>
    </view>
  </view>

  <view class="price_wraper flex_sa">
    <view class="price_li" bindtap="skip" data-url="/pages/distributor/distributor?team_id={{team_id}}">
      <view>{{team_info.amount}}</view>
      <view class="text">总金额</view>
    </view>
    <view class="price_li" bindtap="skip" data-url="/pages/distributor/distributor?team_id={{team_id}}">
      <view>{{team_info.gold_coin}} </view>
      <view class="text">总金币</view>
    </view>
  </view>

  <!-- <view class="price_wraper flex_sa">
    <view class="price_li buy_record">
      <view class="cover">
        <image src="../../images/logo.png"></image>
      </view>
      <view class="text">最大购买</view> 
    </view>
    <view class="price_li buy_record">
      <view class="cover">
        <image src="../../images/logo.png"></image>
      </view>
      <view class="text">最高消费</view>
    </view>
  </view> -->


  <view class="auction_wraper" style="margin-top:20rpx;">
    <view class="auction_box" wx:if="{{room_info.floor_status==1}}">
      <view class="title">地板拍卖</view>
      <view class="auction_ul flex_y flex_sb">
        <view>
          <view>
            <text>地板</text>
            <text class="floor_type" wx:if="{{room_info.floorInfo.purple==1}}">紫</text>
            <text class="floor_type" wx:if="{{room_info.floorInfo.blue==1}}">蓝</text>
            <text class="floor_type" wx:if="{{room_info.floorInfo.green==1}}">绿</text>
          </view>
          <view class="floor_price">售价 {{room_info.floorInfo.price}} {{room_info.floorInfo.currency_type=='2'?'元':'币'}}</view>
        </view>
        <view wx:if="{{floor.is_floor==0}}">
          <view class="btn_sty" bindtap="auction_pay" data-index="-1">购买地板</view>
        </view>
        <view wx:if="{{floor.is_floor==1}}">
          <view class="floor_gm flex_y">
            <image src="{{floor.user.avatar}}"></image>
            <text>{{floor.user.nickname}}</text>
            <text>购买</text>
          </view>
        </view>
        <view wx:if="{{floor.is_floor==3}}">
          <view class="floor_gm flex_y">
            <image src="{{floor.user.avatar}}"></image>
            <text>{{floor.user.nickname}}</text>
            <text>待支付中....</text>

            <text class="sh" wx:if="{{identity==1}}" bindtap="setReview" data-index="-1">审核</text>
          </view>
        </view>
      </view>
    </view>

    <view class="auction_box" wx:if="{{teamInfo.isdel==1}}">
      <view class="title">装备拍卖</view>
      <view class="flex top_bar">
        <view class="top_bar_li flex_vert {{top_bar_index==0?'top_bar_current':''}}" bindtap="sw_bar" data-index="0">
          <view>正在拍卖</view>
        </view>
        <view class="top_bar_li flex_vert {{top_bar_index==1?'top_bar_current':''}}" bindtap="sw_bar" data-index="1">
          <view>交易成功</view>
        </view>
        <view class="top_bar_li flex_vert {{top_bar_index==2?'top_bar_current':''}}" bindtap="sw_bar" data-index="2">
          <view>流拍</view>
        </view>
        <view class="top_bar_li flex_vert {{top_bar_index==3?'top_bar_current':''}}" bindtap="sw_bar" data-index="3">
          <view>{{identity==1?'交易审核':'我的交易'}}</view>
        </view>
      </view>
      <scroll-view class="equip_ul" scroll-y="{{true}}">
        <view wx:if="{{top_bar_index==0}}">
          <block wx:if="{{equip_list.length>0}}">
            <view class="equip_li flex_y" wx:for="{{equip_list}}" wx:key="index">
              <view class="icon">
                <view class="cover">
                  <image src="{{item.equipment_icon}}"></image>
                </view>
              </view>
              <view class="info">
                <view>
                  <text>{{item.equipment_name}}</text>
                  <text class="level">lv{{item.equipment_grade}}</text>
                  <text class="type">{{item.equipment_type}}</text>
                </view>
                <view class="current_price">
                  <text>{{item.is_visit==0?'起拍价':item.user.nickname}} {{item.price}}{{item.currency_type==2?'元':'币'}}</text>
                </view>
              </view>
              <view class="fun">
                <view class="flex_y" wx:if="{{item.end_time>0}}">剩余
                  <van-count-down time="{{item.end_time*1000}}" format="mm:ss" bind:finish="set_equip_finish" data-index="{{index}}" data-id="0" />
                </view>
                <view class="flex_y" wx:if="{{item.end_time==0}}" style="font-size:22rpx;color:#f7b10a;">
                  已结束，计算中
                </view>
                <view class="btn_sty" bindtap="set_pat_popup" data-index="{{index}}">立即拍</view>
              </view>
            </view>
          </block>
          <block wx:else>
            <view class="empty_list">
              还没有添加装备，请努力刷本
            </view>
          </block>
        </view>


        <view wx:if="{{top_bar_index==1}}">
          <block wx:if="{{equip_list.length>0}}">
            <view class="bill_list" wx:for="{{equip_list}}" wx:key="index">
              <view class="flex_y">
                <view class="icon flex-grow-0">
                  <image src="{{item.equipment_icon}}"></image>
                </view>
                <view class="info">
                  <view class="name">{{item.equipment_name}}</view>
                  <view class="configuration">
                    <text>{{item.equipment_type}}</text>
                    <text>lv{{item.equipment_grade}}</text>
                  </view>
                </view>
              </view>
              <view class="pay flex_sb">
                <view class="flex_y">
                  <image src="{{item.user.avatar}}"></image>
                  <text style="margin-left:10rpx; margin-right:10rpx;font-size:20rpx;">{{item.user.nickname}}</text> 拍到
                </view>
                <view>
                  应付金额：
                  <text>{{item.price}}{{item.currency_type==2?'元':'币'}}</text>
                </view>
              </view>
            </view>
          </block>
          <block wx:else>
            <view class="empty_list">
              还没有交易成功的装备
            </view>
          </block>
        </view>

        <view wx:if="{{top_bar_index==2}}">
          <block wx:if="{{equip_list.length>0}}">
            <view class="bill_list" wx:for="{{equip_list}}" wx:key="index">
              <view class="flex_y">
                <view class="icon flex-grow-0">
                  <image src="{{item.equipment_icon}}"></image>
                </view>
                <view class="info">
                  <view class="name">{{item.equipment_name}}</view>
                  <view class="configuration">
                    <text>{{item.equipment_type}}</text>
                    <text>lv{{item.equipment_grade}}</text>
                  </view>
                </view>
              </view>
              <image src="../../images/on_pay.png" class="on_pay"></image>
            </view>
          </block>
          <block wx:else>
            <view class="empty_list">
              还没有流拍的装备
            </view>
          </block>
        </view>

        <view wx:if="{{top_bar_index==3}}">
          <block wx:if="{{trading_list.data.length>0}}">
            <view class="bill_list" wx:for="{{trading_list.data}}" wx:key="index">
              <view class="flex_y">
                <view class="icon flex-grow-0">
                  <image src="{{item.equipment_icon}}"></image>
                </view>
                <view class="info">
                  <view class="name">{{item.equipment_name}}</view>
                  <view class="configuration">
                    <text>{{item.equipment_type}}</text>
                    <text>lv{{item.equipment_grade}}</text>
                  </view>
                </view>
              </view>
              <view class="pay flex_sb">
                <view class="flex_y">
                  <image src="{{item.user.avatar}}"></image>
                  <text style="margin-left:10rpx; margin-right:10rpx;font-size:20rpx;">{{item.user.nickname}}</text> 拍到
                </view>
                <view>
                  应付金额：
                  <text>{{item.price}}{{item.currency_type==2?'元':'币'}}</text>
                </view>
              </view>
              <view class="pay_for flex_y flex_sb">
                <view class="pay_time flex_y" wx:if="{{item.type!=1}}">
                  <image src="../../images/timer.png"></image>
                  支付剩余
                  <van-count-down time="{{item.end_after_pay*1000}}" format="mm:ss" bind:finish="set_equip_finish" data-index="{{index}}" data-id="1" />
                </view>


                <view class="btn" wx:if="{{item.currency_type==1&&item.type==4&&identity==1}}" style="width:180rpx;" bindtap="setReview" data-index="{{index}}">审核</view>
                <view class="btn" wx:if="{{item.currency_type==1&&item.type==4&&identity!=1}}" style="width:180rpx;">待审核</view>
                <view wx:if="{{item.type==3}}">
                  <view class="btn" wx:if="{{item.currency_type==2&&item.end_after_pay!=0}}" bindtap="auction_pay" data-index="{{index}}">去支付</view>
                  <view class="btn" wx:if="{{item.currency_type==1&&item.end_after_pay!=0&&item.is_team==trading_list.is_team}}" style="width:280rpx;" bindtap="auction_pay" data-index="{{index}}">我已向团长支付金币</view>
                  <view class="btn" wx:if="{{item.end_after_pay==0&&item.is_team==trading_list.is_team}}" style="width:280rpx;background-color:#999;background-image:none;">交易已结束，正在计算中</view>
                </view>


                <!-- <view wx:if="{{item.end_after_pay==0&&item.is_team==trading_list.is_team}}">已失效</view> -->
                <view wx:if="{{item.type==1}}">交易成功</view>
                <view class="btn" wx:if="{{item.currency_type==1&&item.type==1&&item.is_team==0}}" style="background-color:#999;background-image:none;">通过审核</view>
              </view>
            </view>
          </block>
          <block wx:else>
            <view class="empty_list">
              还没有拍中的装备，请加油
            </view>
          </block>
        </view>
      </scroll-view>
    </view>
    <view class="auction_box" wx:if="{{teamInfo.isdel==0}}">
      <view class="title">装备拍卖</view>
      <view class="open_game_title">
        副本之路还未开启
      </view>
    </view>
  </view>



  <!-- 底部按钮 -->
  <view class="fun_btn flex_vert" wx:if="{{identity==1&&teamInfo.isdel==0}}">
    <view class="bnt1" bindtap="open_game">开启副本</view>
  </view>
  <view class="fun_btn flex_vert" wx:if="{{identity==1&&teamInfo.isdel==1}}">
    <view class="bnt1 right_border" bindtap="add_equip_btn">添加装备</view>
    <view class="bnt1" bindtap="skip" data-url="/pages/distributor/distributor?team_id={{team_id}}">分配金额</view>
  </view>

  <!-- 查看团队详细资料 -->
  <view class="popup" wx:if="{{group_info_popup}}">
    <view class="popup_bj" bindtap="close_group_popup"></view>
    <view class="popup_box">
      <view class="close" bindtap="close_group_popup">
        <image src="../../images/close.png"></image>
      </view>
      <view class="flex_x popup_title">开团规则</view>
      <scroll-view class="group_box">

        <!-- <view class="li flex">
          <view class="title flex-grow-0">房间名称：</view>
          <view>天使的房间</view>
        </view> -->
        <view class="li flex">
          <view class="title flex-grow-0">房间号：</view>
          <view class="flex_sb group_flex">
            <view>{{room_info.room_num}}</view>
            <view bindtap="copyBtn">复制</view>
          </view>
        </view>
        <view class="li flex">
          <view class="title flex-grow-0">副本：</view>
          <view>{{room_info.transcript_name}}</view>
        </view>
        <view class="li flex" wx:if="{{room_info.yy_room_number.length>0}}">
          <view class="title flex-grow-0">YY房间：</view>
          <view>{{room_info.yy_room_number}}</view>
        </view>
        <view class="li flex">
          <view class="title flex-grow-0">服务器/阵营：</view>
          <view>{{room_info.service_name}}-{{room_info.camp_name}}</view>
        </view>
        <view class="li flex" wx:if="{{room_info.expenditure>0}}">
          <view class="title flex-grow-0">支出：</view>
          <view>{{room_info.expenditure}}{{room_info.subsidy.currency_type==2?'元':room_info.subsidy.currency_type==1?'币':''}}</view>
        </view>
        <view class="li flex">
          <view class="title flex-grow-0">补贴方式：</view>
          <view class="subsidies flex_wrap">
            <view class="type" wx:for="{{room_info.subsidy.subsidy}}" wx:key="index" wx:if="{{item.value.length>0}}">{{item.name}}-{{item.value}}{{room_info.subsidy.status==1?'%-':''}}{{room_info.subsidy.currency_type==2&&room_info.subsidy.status==2?'元':room_info.subsidy.currency_type==1&&room_info.subsidy.status==2?'币':''}}</view>
          </view>
        </view>
        <view class="li flex">
          <view class="title flex-grow-0">分配条件：</view>
          <view>
            <view>DPS需高于第{{room_info.high_dps.key}}名的{{room_info.high_dps.value}}%</view>
            <view>HPS需高于第{{room_info.high_hps.key}}名的{{room_info.high_hps.value}}%</view>
          </view>
        </view>
        <view class="li flex">
          <view class="title flex-grow-0">地板：</view>
          <view>
            <view>{{room_info.floor_status==1?'开启':'关闭'}}</view>
            <view wx:if="{{room_info.floor_status==1}}">
              <text wx:if="{{room_info.floorInfo.purple==1}}">紫、</text>
              <text wx:if="{{room_info.floorInfo.blue==1}}">蓝、</text>
              <text wx:if="{{room_info.floorInfo.green==1}}">绿</text>
              <text style="margin-left:20rpx;">售卖 {{room_info.floorInfo.price}} {{room_info.floorInfo.currency_type==2?'元':'币'}}</text>
            </view>
          </view>
        </view>

        <view class="li flex" wx:if="{{room_info.note.length>0}}">
          <view class="title flex-grow-0">团长备注：</view>
          <view>
            {{room_info.note}}
          </view>
        </view>
      </scroll-view>
    </view>
  </view>
  <!-- 管理团员信息 -->
  <view class="popup" wx:if="{{team_info_popup}}">
    <view class="popup_bj" bindtap="close_team_popup"></view>
    <view class="popup_box">
      <view class="close" bindtap="close_team_popup">
        <image src="../../images/close.png"></image>
      </view>
      <view class="flex_x popup_title">成员信息</view>
      <view class="team_box">
        <scroll-view class="team_ul" scroll-y="{{true}}">
          <view wx:for="{{team_list}}" wx:key="index" wx:for-item="index_item" wx:for-index="index_t">
            <view class="name">{{index_item.name}}</view>
            <view class="equip_li flex_y team_li" wx:for="{{index_item.list}}" wx:key="index">
              <view class="icon">
                <view class="cover">
                  <image src="{{item.avatar}}"></image>
                </view>
              </view>
              <view class="info">
                <view>
                  <text style="color:{{index_item.color}}" class="user_role_name">{{item.user_role_name}}</text>
                  <text class="level">lv{{item.grade}}</text>
                </view>
                <view class="current_price">
                  <text style="color:{{index_item.color}}">{{item.occupation_name}}</text>
                  <text>装等：{{item.equipment_grade}}</text>
                  <text class="" style="{{item.identity==1?'color:#000;font-weight: 600;':''}}">{{item.identityName}}</text>
                </view>
              </view>
              <view class="fun" wx:if="{{identity==1}}">
                <view class="btn_sty" wx:if="{{item.identity==3}}" bindtap="checkTeamMember" data-id="1" data-index_t="{{index_t}}" data-index="{{index}}">通过</view>
                <view class="btn_sty" wx:if="{{item.identity==3}}" bindtap="checkTeamMember" data-id="2" data-index_t="{{index_t}}" data-index="{{index}}">不通过</view>
                <view class="btn_sty" wx:if="{{item.identity==2}}" bindtap="removeTeamMember" data-index_t="{{index_t}}" data-index="{{index}}">踢出</view>
              </view>
            </view>
          </view>


        </scroll-view>
        <view class="funBtn">
          <view wx:if="{{identity==1}}" bindtap="teamLeaderDissolutionTeam">解散团队</view>
          <view wx:if="{{identity!=1}}" bindtap="userQuitTeam">退出</view>
        </view>
      </view>
    </view>
  </view>

  <!-- 添加装备 -->
  <van-popup show="{{equip_popup}}" position="bottom" custom-style="height: 1182rpx;">
    <view class="popup_box boss_box">

      <view class="close" bindtap="close_boss_popup">
        <image src="../../images/close.png"></image>
      </view>
      <view wx:if="{{!select_boss}}">
        <view class="subsidy_title flex_x boss_title">选择boss</view>
        <scroll-view scroll-y="{{true}}" class="scroll_h">
          <view class="ul_box">
            <view wx:for="{{boss_list}}" wx:key="index" class="ul_li" data-index="{{index}}" bindtap="set_select_boss">
              <view class="boss_name">{{item.name}}</view>
              <view class="boss_english">{{item.yname}}</view>
            </view>
          </view>
        </scroll-view>
      </view>


      <view wx:if="{{select_boss}}">
        <view class="subsidy_title flex_x boss_title">{{boss_list[boss_index].name}}</view>

        <view class="ul_box">
          <view class="ul_li flex_sa">
            <view class="select_icon">
            </view>
            <view class="eqiop_icon">
              <view>图标</view>
            </view>
            <view class="eqiop_name">
              <view>名称</view>
            </view>
            <view class="l1_eqiop">
              <view>等级</view>
            </view>
            <view class="l1_eqiop">
              <view>类型</view>
            </view>
            <view class="eqiop_type">
              <view>币种</view>
            </view>
            <view class="set_up_input">
              <view>价格</view>
            </view>
          </view>
        </view>
        <scroll-view scroll-y="{{true}}" class="scroll_h boss_equip_h">
          <view class="ul_box">
            <view wx:for="{{boss_list[boss_index].equipment}}" wx:key="index" class="ul_li flex_sa flex_y boss_equip">
              <!-- <view class="boss_name">{{item.name}}</view>
                    <view class="boss_english">{{item.yname}}</view> -->
              <view class="select_icon">
                <view class="icon_sty" bindtap="set_select_equip" data-index="{{index}}">
                  <image src="../../images/hook.png" wx:if="{{item.is_select}}"></image>
                </view>
              </view>
              <view class="eqiop_icon">
                <image src="{{item.icon}}"></image>
              </view>
              <view class="eqiop_name">
                <view>{{item.equipmentChineseName}}</view>
                <view class="english_name">{{item.equipmentEnglishName}}</view>
              </view>
              <view class="l1_eqiop">
                <view>{{item.grade}}</view>
              </view>
              <view class="l1_eqiop li_eqiop_type">
                <view>{{item.type}}</view>
              </view>
              <view class="eqiop_type">
                <image src="../../images/drop.png" class="drop"></image>
                <picker bindchange="auction_typeChange" data-index="{{index}}" value="{{item.currency_type}}" range="{{auction_type}}">
                  <view class="auction_type_picker">
                    {{auction_type[item.currency_type]}}
                  </view>
                </picker>

              </view>
              <view class="set_up_input">
                <!-- <view>10</view> -->
                <input type="number" class="set_up" value="{{item.clap_price}}" bindinput="set_price" data-index="{{index}}"></input>
              </view>
            </view>
          </view>
        </scroll-view>


        <view class="set_boss_equip">
          <view class="set_equip_up flex_sa flex_y">
            <view class="set_hook">
              <view class="icon_sty" bindtap="set_select_equip" data-index="-1">
                <image src="../../images/hook.png" wx:if="{{check_all}}"></image>
              </view>
              <text>全选</text>
            </view>
            <view class="li_qu"></view>
            <view class="l1_eqiop eqiop_type">
              <image src="../../images/drop.png" class="drop"></image>
              <picker bindchange="auction_typeChange" data-index="-1" value="{{item.currency_type}}" range="{{auction_type}}">
                <view class="auction_type_picker">
                  {{auction_type[auction_typeIndex]}}
                </view>
              </picker>

            </view>
            <view class="l1_eqiop set_up_input">
              <!-- <view>10</view> -->
              <input type="number" class="set_up" bindinput="set_check_price"></input>
            </view>
          </view>
          <view class="set_boss_equip_btn" bindtap="add_equip">
            添加
          </view>
        </view>
      </view>

    </view>

  </van-popup>



  <!-- 设置拍卖时间 -->
  <view class="popup" wx:if="{{add_equip_popup}}">
    <view class="popup_bj"></view>
    <view class="popup_box">
      <view class="close" bindtap="close_add_popup">
        <image src="../../images/close.png"></image>
      </view>
      <view class="flex_x popup_title">设置拍卖时间</view>

      <view class="group_box">
        <view class="flex_y floor_li">
          <view>每次加价</view>
          <view>
            <input type="number" placeholder="每次加价" placeholder-style="color:#190f00" class="floorEquipIpunt" bindinput="equipIpunt"></input>
          </view>
        </view>
        <view class="flex_y floor_li">
          <view>拍卖时间</view>
          <view class="time_wap">
            <picker bindchange="floor_time_Change" data-id="wap" value="{{floor_time_wap_index}}" range="{{floor_time_wap}}">
              <view class="floor_time">
                {{floor_time_wap[floor_time_wap_index]}} 分钟
              </view>
            </picker>
            <image src="../../images/drop1.png" class="drop"></image>
          </view>
        </view>
        <view class="flex_y floor_li">
          <view>支付时间</view>
          <view class="time_wap">
            <picker bindchange="floor_time_Change" data-id="pay" value="{{floor_time_pay_index}}" range="{{floor_time_pay}}">
              <view class="floor_time">
                {{floor_time_pay[floor_time_pay_index]}} 分钟
              </view>
            </picker>
            <image src="../../images/drop1.png" class="drop"></image>
          </view>
        </view>
        <view class="set_floor_btn" bindtap="set_eqiop_btn">
          确定
        </view>

      </view>
    </view>
  </view>


  <!-- 立即拍卖 详情-->
  <view class="popup" wx:if="{{pat_popup}}">
    <view class="popup_bj"></view>
    <view class="popup_box">
      <view class="close" bindtap="close_pat_popup">
        <image src="../../images/close.png"></image>
      </view>

      <view class="flex_x popup_title pat_title">立即拍</view>
      <view class="pat_box">
        <view class="flex">
          <view class="cover">
            <image src="{{now_equip_info.auction_info.equipment_icon}}"></image>
          </view>
          <view class="info">
            <view>
              <text>{{now_equip_info.auction_info.equipment_name}}</text>
              <text class="type">lv{{now_equip_info.auction_info.equipment_grade}}</text>
              <text class="type">{{now_equip_info.auction_info.equipment_type}}</text>
            </view>
            <view class="flex_sb pat_count_down">
              <view>{{now_equip_info.auction_info.is_visit==1?'当前最高':'起拍价'}} {{now_equip_info.auction_info.equipment_price}} {{now_equip_info.auction_info.currency_type==2?'元':'币'}}</view>
              <view class="flex timer">剩余时间
                <van-count-down time="{{now_equip_info.auction_info.end_time*1000}}" format="mm:ss" bind:finish="set_equip_finish" data-index="{{index}}" />
              </view>
            </view>
          </view>
        </view>
        <view class="record_box">
          <view class="title">拍卖纪录</view>
          <scroll-view class="scroll_record" scroll-y="{{true}}">
            <view class="flex_y record_li" wx:for="{{now_equip_info.record_log}}" wx:key="index" wx:if="{{now_equip_info.record_log.length>0}}">
              <view class="cover_box">
                <view class="cover">
                  <image src="{{item.user.avatar}}"></image>
                </view>
              </view>
              <view class="name">{{item.user.nickname}}</view>
              <view class="timer">{{item.create_time}}</view>
              <view class="price">{{item.price}}{{item.currency_type==2?'元':'币'}}</view>
            </view>
            <view class="flex_y record_li" wx:if="{{now_equip_info.record_log.length==0}}">
              <view>
                暂时无人竞拍
              </view>
            </view>
          </scroll-view>
        </view>

        <view class="pat_price_box">
          <input type="number" class="pat_price" placeholder="请输入参与的价格" bindinput="pat_price" value="{{now_pat_price}}"></input>
          <view class="note">每次加价不能少于{{now_equip_info.auction_info.add_price}}{{now_equip_info.auction_info.currency_type==2?'元':'币'}}</view>
        </view>

        <view class="set_pat" bindtap="add_auction">立即参拍</view>
      </view>
    </view>
  </view>
</view>